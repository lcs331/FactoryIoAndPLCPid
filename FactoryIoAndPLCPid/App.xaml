<prism:PrismApplication x:Class="FactoryIoAndPLCPid.App"
                        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                        xmlns:prism="http://prismlibrary.com/">
    <!-- Prism 不用 StartupUri，Shell 是在 App.xaml.cs 的 CreateShell() 返回的 -->

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resource/IconDic.xaml"/>
                <ResourceDictionary Source="/Resource/Styles/ComboBoxStyle.xaml"/>
                <ResourceDictionary Source="/Resource/CapsuleTextLblDic.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>

</prism:PrismApplication>

